<style></style>
<section class="section section-motion" data-name="motion" data-anim-scroll-group="motion" data-analytics-section-engagement="name:motion">
    <div class="section-main-content">
        <div class="section-media image-center" data-component-list="MotionHero">
            <div class="video-container" role="img" aria-label="Motion watch hardware water animation">
                <div class="visuallyhidden focus-item" tabindex="-1"></div>
                <figure class="start-frame image-motion" data-anim-lazy-image></figure>
                <video class="media-video" muted="" playsinline="true" aria-hidden="true" data-video-source-basepath="/build/website/mp4/motion/" data-video-source-retina-enabled="true"></video>
                <figure class="end-frame" data-anim-lazy-image></figure>
            </div>
        </div>
        <div class="section-block">
            <h2 class="section-block-headline typography-section-headline" data-text-zoom-max="1.75">
                {% if content.title %}<span class="headline {% if content.raidbow_txt %}raidbow-txt{% endif %}">{{ content.title|raw }}</span>{% endif %}
                {% if content.subtitle %}<span class="subhead">{{ content.subtitle|raw }}</span>{% endif %}
            </h2>
            {% if content.description %}<p class="section-block-intro typography-section-intro">{{ content.description|raw }}</p>{% endif %}
        </div>
        <div class="motion-sticky-block" data-anim-keyframe='{
			"anchors":[".motion-sticky-block"],
			"start":"a0t - 100vh",
			"end":"a0b",
			"cssClass":"will-change",
			"toggle":true,
			"disabledWhen":["static-layout", "text-zoom"]
		}'>
            <div class="image-container" data-anim-keyframe-1='{
				"anchors":[".subsection-scroll", ".subsection-always-on", ".image-scroll-end"],
				"start":"a0t",
				"end":"a1t - 50vh",
				"y":[null, "-240px"],
				"ease":"0.2",
				"disabledWhen": ["static-layout", "text-zoom"],
				"breakpointMask": "L"
			}' data-anim-keyframe-2='{
				"anchors":[".subsection-scroll", ".subsection-always-on", ".image-scroll-end"],
				"start":"a0t",
				"end":"a1t - 50vh",
				"y":[null, "-120px"],
				"ease":"0.2",
				"disabledWhen": ["static-layout", "text-zoom"],
				"breakpointMask": "M"
			}' data-anim-keyframe-3='{
				"anchors":[".subsection-scroll", ".subsection-always-on", ".image-scroll-end"],
				"start":"a0t - 20vh",
				"end":"a1t",
				"ease":"0.2",
				"y":["0px", null],
				"disabledWhen": ["static-layout", "text-zoom"],
				"breakpointMask": "S"
			}'>
                <figure class="image-scroll-anim scroll-one" data-anim-keyframe-1='{
					"anchors":[".subsection-scroll", ".subsection-always-on", ".image-scroll-start", ".image-scroll-end"],
					"start":"a0t",
					"end":"a1t - 50vh",
					"x":["a2l - 186px", "a3l + 180px"],
					"ease":"0.2",
					"disabledWhen": ["static-layout", "text-zoom"],
					"breakpointMask": "L"
				}' data-anim-keyframe-2='{
					"anchors":[".subsection-scroll", ".subsection-always-on", ".image-scroll-start", ".image-scroll-end"],
					"start":"a0t",
					"end":"a1t - 50vh",
					"x":["a2l - 168px", "a3l + 187px"],
					"ease":"0.2",
					"disabledWhen": ["static-layout", "text-zoom"],
					"breakpointMask": "M"
				}' data-anim-keyframe-3='{
					"anchors":[".subsection-scroll", ".subsection-always-on", ".image-scroll-start", ".image-scroll-end"],
					"start":"a0t - 50vh",
					"end":"a1t",
					"x":["a2l + 177px", "a3l + 22px"],
					"ease":"0.2",
					"disabledWhen": ["static-layout", "text-zoom"],
					"breakpointMask": "S"
				}' data-anim-lazy-image>
                    <div data-component-list="MotionHardware" data-anim-lazy-image>
                        <div class="video-container motion-club" role="img" aria-label="Club screen animation">
                            <div class="visuallyhidden focus-item" tabindex="-1"></div>
                            <figure class="start-frame image-video" data-anim-lazy-image></figure>
                            <video class="media-video" muted="" playsinline="true" aria-hidden="true" data-video-source-basepath="/build/website/mp4/motion/club/" data-video-source-retina-enabled="true"></video>
                            <figure class="end-frame" data-anim-lazy-image></figure>
                        </div>
                    </div>
                    <div data-component-list="MotionHardware" data-anim-lazy-image>
                        <div class="video-container motion-search" role="img" aria-label="Search screen animation">
                            <div class="visuallyhidden focus-item" tabindex="-1"></div>
                            <figure class="start-frame image-video" data-anim-lazy-image></figure>
                            <video class="media-video" muted="" playsinline="true" aria-hidden="true" data-video-source-basepath="/build/website/mp4/motion/search/" data-video-source-retina-enabled="true"></video>
                            <figure class="end-frame" data-anim-lazy-image></figure>
                        </div>
                    </div>
                    <div data-component-list="MotionHardware" data-anim-lazy-image>
                        <div class="video-container motion-users" role="img" aria-label="Users screen animation">
                            <div class="visuallyhidden focus-item" tabindex="-1"></div>
                            <figure class="start-frame image-video" data-anim-lazy-image></figure>
                            <video class="media-video" muted="" playsinline="true" aria-hidden="true" data-video-source-basepath="/build/website/mp4/motion/users/" data-video-source-retina-enabled="true"></video>
                            <figure class="end-frame" data-anim-lazy-image></figure>
                        </div>
                    </div>
                </figure>
                <div class="image-scroll scroll-one image-scroll-1">
                    <figure class="image-scroll-on scroll-one image-scroll-start" data-anim-lazy-image></figure>
                </div>
                <div class="image-scroll scroll-one image-scroll-3">
                    <figure class="image-scroll-on scroll-one image-scroll-end" data-anim-lazy-image></figure>
                </div>
            </div>
            <div class="subsections" data-component-list="MotionCopy">
                <div class="subsection subsection-scroll">
                    <div class="copy-block copy-block-scroll anim-text-animate end" data-build-in-start="a0t - 50vh" data-build-in-start-s="a0b - 75vh" data-build-in-end="a0t" data-build-in-end-s="a0b">
                        {% if content.workouts_title %}<h3 class="copy-block-headline typography-copy-block">{{ content.workouts_title|raw }}<br /></h3>{% endif %}
                        {% if content.workouts_desc %}
                        <p class="copy-block-copy typography-copy-block">
                            {{ content.workouts_desc|raw }}
                        </p>
                        {% endif %}
                    </div>
                </div>
                <div class="subsection subsection-always-on">
                    <div class="copy-block copy-block-always-on anim-text-animate end" data-build-in-start="a0t - 50vh" data-build-in-start-s="a0b - 75vh" data-build-in-end="a0t" data-build-in-end-s="a0b">
                        {% if content.alwayson_title %}<h3 class="copy-block-headline typography-copy-block">{{ content.alwayson_title|raw }}<br /></h3>{% endif %}
                        {% if content.alwayson_desc %}
                        <p class="copy-block-copy typography-copy-block">
                            {{ content.alwayson_desc|raw }}
                        </p>
                        {% endif %}
                    </div>
                </div>
                <div class="subsection subsection-altimeter">
                    <div class="copy-block copy-block-altimeter anim-text-animate end" data-build-in-start="a0t - 50vh" data-build-in-start-s="a0b - 75vh" data-build-in-end="a0t" data-build-in-end-s="a0b">
                        {% if content.altimeter_title %}<h3 class="copy-block-headline typography-copy-block">{{ content.altimeter_title|raw }}<br /></h3>{% endif %}
                        {% if content.altimeter_desc %}
                        <p class="copy-block-copy typography-copy-block">
                            {{ content.altimeter_desc|raw }}
                        </p>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

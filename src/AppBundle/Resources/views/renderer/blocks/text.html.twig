{% set propertyName = propertyName|default('blocks') %}
<div rel="{{ propertyName }}" typeof="block">
    <div property="background" class="{{ block.background ? 'background' : '' }}">
        <div property="text" class="text">
            {% if block.text|default('') %}
                {{ block.text|raw }}
            {% endif %}
        </div>
    </div>
</div>
